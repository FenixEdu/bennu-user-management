package pt.ist.bennu.user.management;

class UserLoginPeriod {
    LocalDate beginDate (REQUIRED);
    LocalDate endDate;
}

relation UserHasLoginPeriods {
    .pt.ist.bennu.core.domain.User playsRole user {
        multiplicity 1..1;
    }
    UserLoginPeriod playsRole loginPeriod {
        multiplicity *;
    }
}